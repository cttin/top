<!--pages/login/login.wxml-->
<view class='login show}}'>
  <view class='login-img'>
    <image src='/images/logo.png'></image>
  </view>
  <view class='login-des padding-top-xl'>
    <text>小程序需要获取你的公开信息（昵称、头像等），才能正常使用</text>
  </view>
  <view class='padding-xl'>
    <button class='cu-btn  bg-green shadow lg block' open-type="getUserInfo" bindgetuserinfo="onGetUserInfo">同意授权</button>
  </view>
</view>
